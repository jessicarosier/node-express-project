<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
  <div id="user-wrapper">
    <ul>
        <% users.forEach((user) => { %>
            <li>
              <div>
                <h3><%= user.name %></h3>
                <p><%= user.email %></p>
                <a href="<%= user.website%>" target="_blank"><%= user.website%></a>
              </div>
            </li>
        <% }) %>
    </ul>

  </div>
  </body>
</html>
